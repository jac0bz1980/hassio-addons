{
  "name": "Sherpa-OnnxAsr-tiny",
  "version": "latest",
  "slug": "sherpa-onnx-asr",
  "description": "本地离线语音模型，完整版占用1GB内存，这个版本是tiny版，方便小内存设备安装。",
  "url": "https://github.com/yaming116",
  "arch": ["amd64", "aarch64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "image": "yaming116/sherpa-onnx-asr-tiny",
  "ports": {
    "5001/tcp": 5001
  },
  "ports_description": {
    "5001/tcp": "ASR Web interface"
  },
  "map": {
    "config": {
      "path": "/data",
      "readonly": false
    }
  },
  "environment": {},
  "options": {
    "language": "zh-CN"
  },
  "schema": {
    "language": "str"
  },
  "ingress": true,
  "ingress_stream": true,
  "ingress_port": 5001
}
